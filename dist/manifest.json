{
     "manifest_version": 2,
     "name": "A extension for SAU ADs",
     "version": "0.0.2",
     "options_page": "index.html",
     "background": {
		 "scripts": ["./loader.js"]
	 },
	 "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "*://*/*"
     ],
	 "icons": 
     {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
     },
     "page_action": {
          "default_icon": {
               "19": "icon.png"
          },
          "default_title": "SAU ADBlocker Extension"
     }
}
